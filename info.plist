				<string>Search for a repo that has a `.git` file on your computer</string>
				<key>script</key>
				<string>#!/usr/bin/swift

import Foundation

// MARK: - Configuration
let homeDirectory = FileManager.default.homeDirectoryForCurrentUser.path
let startsWith = CommandLine.arguments.count &gt; 1 ? CommandLine.arguments[1] : ""
let tmpDir = NSTemporaryDirectory()
let lockFile = tmpDir + "cached_repos.lock"
let cachedReposFile = tmpDir + "cached_repos"
let shallowMaxDepth = ProcessInfo.processInfo.environment["scan_depth"].flatMap { Int($0) } ?? 3

// MARK: - Helper Functions

func isProcessRunning(_ pid: Int32) -&gt; Bool {
    return kill(pid, 0) == 0
}

func findGitRepos(in directory: String) -&gt; [String] {
    var repos: [String] = []
    let fileManager = FileManager.default
    
    guard let enumerator = fileManager.enumerator(
        at: URL(fileURLWithPath: directory),
        includingPropertiesForKeys: [.isDirectoryKey, .nameKey],
        options: [.skipsHiddenFiles]
    ) else {
        return repos
    }
    
    for case let fileURL as URL in enumerator {
        guard let resourceValues = try? fileURL.resourceValues(forKeys: [.isDirectoryKey, .nameKey]),
              let isDirectory = resourceValues.isDirectory,
              let name = resourceValues.name else {
            continue
        }
        
        if isDirectory &amp;&amp; name == ".git" {
            let repoPath = fileURL.deletingLastPathComponent().path
            repos.append(repoPath)
            enumerator.skipDescendants()
        }
    }
    
    return repos
}

func updateCache() {
    let fileManager = FileManager.default
    
    // Check if lock file exists and belongs to this process
    if fileManager.fileExists(atPath: lockFile) {
        if let storedPid = try? String(contentsOfFile: lockFile, encoding: .utf8).trimmingCharacters(in: .whitespacesAndNewlines),
           let pid = Int32(storedPid),
           pid != getpid() {
            exit(0)
        }
    }
    
    // Find all repos
    let repos = findGitRepos(in: homeDirectory)
    
    // Write to cache file
    let reposString = repos.joined(separator: "\n")
    try? reposString.write(toFile: cachedReposFile, atomically: true, encoding: .utf8)
    
    // Remove lock file
    try? fileManager.removeItem(atPath: lockFile)
}

func cacheUpdating() -&gt; Bool {
    let fileManager = FileManager.default
    
    guard fileManager.fileExists(atPath: lockFile) else {
        return false
    }
    
    if let storedPid = try? String(contentsOfFile: lockFile, encoding: .utf8).trimmingCharacters(in: .whitespacesAndNewlines),
       let pid = Int32(storedPid) {
        return isProcessRunning(pid)
    }
    
    return false
}

func getCachedRepos() -&gt; [String] {
    guard let content = try? String(contentsOfFile: cachedReposFile, encoding: .utf8) else {
        return []
    }
    return content.components(separatedBy: .newlines).filter { !$0.isEmpty }
}

func findGitReposShallow(in directory: String, maxDepth: Int) -&gt; [String] {
    var repos: [String] = []
    let fileManager = FileManager.default
    
    func search(at path: String, depth: Int) {
        guard depth &lt;= maxDepth else { return }
        
        guard let contents = try? fileManager.contentsOfDirectory(atPath: path) else {
            return
        }
        
        for item in contents {
            let itemPath = (path as NSString).appendingPathComponent(item)
            var isDir: ObjCBool = false
            
            guard fileManager.fileExists(atPath: itemPath, isDirectory: &amp;isDir) else {
                continue
            }
            
            if isDir.boolValue {
                if item == ".git" {
                    repos.append(path)
                } else if !item.hasPrefix(".") {
                    search(at: itemPath, depth: depth + 1)
                }
            }
        }
    }
    
    search(at: directory, depth: 1)
    return repos
}

// MARK: - Main Logic

let fileManager = FileManager.default
var lockPid = ""

if fileManager.fileExists(atPath: lockFile) {
    lockPid = (try? String(contentsOfFile: lockFile, encoding: .utf8).trimmingCharacters(in: .whitespacesAndNewlines)) ?? ""
}

// Check if cache needs updating
let cacheExists = fileManager.fileExists(atPath: cachedReposFile)
var cacheOld = false

if cacheExists {
    if let attributes = try? fileManager.attributesOfItem(atPath: cachedReposFile),
       let modificationDate = attributes[.modificationDate] as? Date {
        let dayInSeconds: TimeInterval = 24 * 60 * 60
        cacheOld = Date().timeIntervalSince(modificationDate) &gt; dayInSeconds
    }
}

// Update cache if needed
if !cacheExists || cacheOld {
    let pid = getpid()
    try? "\(pid)".write(toFile: lockFile, atomically: true, encoding: .utf8)
    
    // Fork process for cache update
    DispatchQueue.global(qos: .background).async {
        updateCache()
    }
}

// Get repos
var repos: [String] = []

if cacheExists {
    repos = getCachedRepos()
} else {
    repos = findGitReposShallow(in: homeDirectory, maxDepth: shallowMaxDepth)
}

// Filter by startsWith if provided
if !startsWith.isEmpty {
    repos = repos.filter { 
        ($0 as NSString).lastPathComponent.lowercased().hasPrefix(startsWith.lowercased())
    }
}

// MARK: - JSON Output

struct AlfredItem: Codable {
    let arg: String
    let uid: String
    let subtitle: String
    let title: String
    let autocomplete: String
    let variables: [String: String]
}

struct AlfredOutput: Codable {
    let items: [AlfredItem]
}

var items: [AlfredItem] = []

for repoPath in repos {
    let directoryName = (repoPath as NSString).lastPathComponent
    let item = AlfredItem(
        arg: directoryName,
        uid: directoryName,
        subtitle: repoPath,
        title: directoryName,
        autocomplete: directoryName,
        variables: [
            "repo_path": repoPath,
            "directory_name": directoryName,
            "lock_pid": lockPid,
            "cache_dir": cachedReposFile
        ]
    )
    items.append(item)
}

let output = AlfredOutput(items: items)

let encoder = JSONEncoder()
encoder.outputFormatting = .prettyPrinted

if let jsonData = try? encoder.encode(output),
   let jsonString = String(data: jsonData, encoding: .utf8) {
    print(jsonString)
} else {
    print("{\"items\": []}")
}</string>
